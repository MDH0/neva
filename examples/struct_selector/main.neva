import { fmt }

type User struct {
    name string
    pet Pet
}

type Pet struct { name string }

def Main(start) (stop) {
    GetUser, fmt.Println
    ---
    :start -> getUser -> .pet.name -> println -> :stop
}

const user User = {
    name: 'John',
    pet: { name: 'Charley' }
}

def GetUser(sig) (u User) {
    :sig -> $user -> :u
}
