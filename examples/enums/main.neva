flow Main(start any) (stop any) {
    DayPrinter
    ---
    :start -> (Day::Friday -> dayPrinter -> :stop)
}

type Day enum {
    Monday,
    Tuesday,
    Wednesday,
    Thursday,
    Friday,
    Saturday,
    Sunday
}

flow DayPrinter(day Day) (sig) {
    Println
    ---
    :day -> println -> :sig
}
