import { streams }

const strings list<string> = ['a', 'b', 'c']

flow Main(start) (stop) {
  nodes {
    r1 Range,
    Iter<string>
    streams.Zip<int, string>
    For{Println}
    Wait
  }
  :start -> [
    (0 -> r1:from),
    (10 -> r1:to)
  ]
  r1 -> zip:first
  $strings -> iter -> zip:second
  zip -> for -> wait -> :stop
}