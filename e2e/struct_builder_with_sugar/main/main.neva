type User struct {
    age int
    name string
}

// FIXME - cannot use 'struct' as a node name

flow Main(start any) (stop any) {
    Println
    builder Struct<User>
    ---
    :start -> [
        { 'John' -> builder:name },
        { 32 -> builder:age }
    ]
    builder:msg -> println:data
    println:sig -> :stop
}