import { lists }

const lst list<bool> = [true, false]

flow Main(start) (stop) {
	ListToStream<bool>, ForEach<bool>{MapAndPrint}, Wait
	---
	:start -> ($lst -> listToStream -> forEach -> wait -> :stop)
}

flow MapAndPrint(data bool) (sig any) {
	Ternary, Println
	---
	:data -> ternary:if
	1 -> ternary:then
	0 -> ternary:else
	ternary -> println -> :sig
}
