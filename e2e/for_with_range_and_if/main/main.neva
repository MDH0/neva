import { lists }

const lst list<bool> = [true, false]

flow Main(start) (stop) {
	ListToStream<bool>, Map<bool, int>{Mapper}, For{Println}, Wait
	---
	:start -> ($lst -> listToStream -> map -> for -> wait -> :stop)
}

flow Mapper(data bool) (res int) {
	Ternary<int>
	---
	:data -> ternary:if
	1 -> ternary:then
	0 -> ternary:else
	ternary -> :res
}
