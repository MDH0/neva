import { lists }

const lst list<bool> = [true, false]

flow Main(start) (stop) {
	nodes { Iter<bool>, Map<bool, int>{Mapper}, For{Println} }
	:start -> ($lst -> iter -> map -> for -> :stop)
}

flow Mapper(data bool) (res int) {
	nodes { Cond<int> }
	:data -> cond:if
	1 -> cond:then
	0 -> cond:else
	cond -> :res
}
