import { lists }

const lst list<bool> = [true, false]

def Main(start) (stop) {
	ListToStream<bool>, For<bool>{PrintAsNum}, Wait
	---
	:start -> { $lst -> listToStream -> for -> wait -> :stop }
}

def PrintAsNum(data bool) (sig any) {
	Ternary, Println
	---
	:data -> ternary:if
	1 -> ternary:then
	0 -> ternary:else
	ternary -> println -> :sig
}
