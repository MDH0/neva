import { fmt }

def Main(start any) (stop any) {
    sub1 SubComponent
    sub2 SubComponent
    panic Panic
    ---
    :start -> 'Hi, Neva!' -> sub1:data
    sub1:stop-> :stop
    '1' -> sub2
    [sub1:err, sub2:err] -> panic
}

def SubComponent(data string) (stop any, err error) {
    println fmt.Println?
    ---
    :data -> println:data
    println:res -> :stop
}
