import { fmt }

def Main(start any) (stop any) {
    second_flow Secondflow {
        dep_node fmt.Println<any>
    }
    ---
    :start -> second_flow:msg
    second_flow:msg -> :stop
}

def Secondflow (msg any) (msg any) {
    dep_node IPrinter<any>
    ---
    :msg -> dep_node:data
    dep_node:sig -> :msg
}

interface IPrinter<T>(T) (T)
